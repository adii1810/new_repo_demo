    .progress-bar {
        cursor: pointer;
        user-select: none;
    }

    .progress-bar {
        background-color: #e1e1e1;
        height: 7px;
        overflow: hidden;
        position: absolute;
        left: 50%;
        bottom: calc(50% + 7px);
        width: 100%;
        z-index: -1;
    }

    .progress-bar--success {
        background-color: #027f00;
    }

    .progress-bar__bar {
        background-color: #e1e1e1;
        bottom: 0;
        left: 0;
        position: absolute;
        right: 0;
        top: 0;
